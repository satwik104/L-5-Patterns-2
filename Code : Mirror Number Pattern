#include<iostream>
using namespace std;


int main(){

       /*  Read input as specified in the question.
	* Print output as specified in the question.
	*/
  int i,j,k,x,n;
    cin>>n;
    for(i=1;i<=n;i++)
    {
        x=1;
        for(k=1;k<=n-i;k++)
            cout<<" ";
        for(j=1;j<=i;j++)
        {
            cout<<x;
            x++;
        }
        cout<<endl;
    }
    return 0;
}


