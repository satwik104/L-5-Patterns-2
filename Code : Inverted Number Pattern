#include<iostream>
using namespace std;


int main(){

       /*  Read input as specified in the question.
	* Print output as specified in the question.
	*/
  int i,j,x,n;
    cin>>n;
    for(i=1;i<=n;i++)
    {
        x=n-i+1;
        for(j=i;j<=n;j++)//for(j=1;j<=n-i+1;j++)
            cout<<x;
        cout<<endl;
    }
 return 0;   
}


